{
  "name": "Cabbage's Anti-Procrastinator",
  "version": "1.3.37",
  "description": "Checks URL and redirects as required. Fuck reddit!",
  "background": { 
     "page":"bg.html"
     },
   "manifest_version": 3,
   "content_scripts": [
   {
     "matches": ["http://*/*", "https://*/*"],
     "js": ["content.js"]
   }
   ],
   
  "permissions": ["tabs"],
  
  "chrome_url_overrides" : {
    "newtab": "redirect.html"
  },
  
  "icons": { "128": "icon.png"
            }

}

